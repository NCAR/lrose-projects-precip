    required = TRUE
  }
  ,
  {
    xml_tag_list = "<ClutterStats><meanDbmhcWeak>",
    specify_label = TRUE,
    label = "meanDbmhcWeak",
    required = TRUE
  }
  ,
  {
    xml_tag_list = "<ClutterStats><meanDbmvcWeak>",
