#! /bin/csh -f

# SPOL QC processing for PRECIP
#
# Run Dsr2Radx
# Reads FMQ from Iq2Dsr.qc.sband
# Writes sband CfRadial data

source ~/git/lrose-projects-precip/projDir/system/params/project_info.precip_qc

cd $PROJ_DIR/qc/params

snuff "Dsr2Radx -params Dsr2Radx.qc.sband"

Dsr2Radx -params Dsr2Radx.qc.sband -debug >& \
    $LOG_DIR/Dsr2Radx.qc.sband.log &

tail -f $LOG_DIR/Dsr2Radx.qc.sband.log




